{
  "env": {
    "node": true,
    "mocha": true
  },
  "extends": "airbnb/base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "noUnderscoreDangle": false
    }
  },
  "rules": {
    "no-underscore-dangle": 0
  }
}
